`timescale 1ns/100ps
module Multiplicador_TB();

wire [7:0] Produto;
wire Idle;
wire Done;
reg [3:0] OperandoMultiplicador;
reg [3:0] Multiplicando;
reg Clk, St;

integer i,j;

Multiplicador DUT(
	.OperandoMultiplicador(OperandoMultiplicador),
	.Multiplicando(Multiplicando),
	.St(St),
	.Clk(Clk),
	.Done(Done),
	.Idle(Idle),
	.Produto(Produto)
);

	initial begin
		forever #40 Clk = ~Clk;
	end
	
	initial begin
		Clk = 0;
		St  = 1;
		
		OperandoMultiplicador = 1101;
		Multiplicando = 1011;
		
		#40;
		St = 0;
	end
	
	initial begin
		#1200 $stop;
	end
	
endmodule